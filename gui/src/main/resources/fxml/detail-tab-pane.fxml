                                   <ChoiceBox prefHeight="25.0" prefWidth="70.0"/>
                                </children>
                                <effect>
                                   <DropShadow height="15" width="15">
                                      <color>
                                         <Color red="0.3" green="0.3" blue="0.3" opacity="0.7" />
                                      </color>
                                   </DropShadow>
                                </effect>
                            </HBox>
                        </AnchorPane>
