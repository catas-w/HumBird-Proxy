                         <!--       </color> -->
                         <!--    </InnerShadow> -->
                         <!-- </effect> -->
                        </AnchorPane>
                    </TitledPane>
                    <TitledPane fx:id="reqPayloadPane" animated="false" maxHeight="Infinity" text="Payload" VBox.vgrow="ALWAYS">
