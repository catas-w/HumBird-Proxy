<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox minWidth="500.0" prefHeight="650.0" prefWidth="1000.0" style="-fx-border-radius: 5;"
      xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.catas.wicked.proxy.gui.controller.AppController">
    <stylesheets>
        <URL value="@../css/app.css"/>
        <URL value="@../css/common-design.css"/>
    </stylesheets>
    <!--<ToolBar style="-fx-background-color: TRANSPARENT;" VBox.vgrow="NEVER">-->
    <!--    <HBox>-->
    <!--        <Button fx:id="closeBtn" text="X"/>-->
    <!--    </HBox>-->
    <!--</ToolBar>-->
    <HBox prefHeight="547.0" prefWidth="900.0" VBox.vgrow="ALWAYS">
        <fx:include source="button-bar.fxml"/>
        <SplitPane dividerPositions="0.3" focusTraversable="true" minWidth="500.0" prefHeight="547.0" prefWidth="838.0"
                   style="-fx-background-color: TRANSPARENT;" HBox.hgrow="ALWAYS">
            <fx:include source="request-view-pane.fxml"/>
            <fx:include source="detail-tab-pane.fxml"/>
        </SplitPane>
    </HBox>
    <HBox id="HBox" alignment="CENTER_LEFT" minHeight="20.0" prefHeight="20.0" prefWidth="900.0" spacing="5.0"
          style="-fx-background-color: TRANSPARENT; -fx-border-color: #efefef;" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
        </padding>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Wicked Proxy" HBox.hgrow="ALWAYS">
            <font>
                <Font size="11.0" fx:id="x3"/>
            </font>
            <textFill>
                <Color red="0.625" green="0.625" blue="0.625" fx:id="x4"/>
            </textFill>
            <HBox.margin>
                <Insets left="5.0"/>
            </HBox.margin>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS"/>
        <Label font="$x3" maxWidth="-1.0" text="v1.1.3" textFill="$x4" HBox.hgrow="NEVER"/>
    </HBox>
</VBox>
