<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.Pane?>
<HBox xmlns="http://javafx.com/javafx/19" prefHeight="40.0">
    <JFXButton text="Reset" styleClass="reset-btn">
        <HBox.margin>
            <Insets left="10.0" />
        </HBox.margin>
    </JFXButton>
    <Pane HBox.hgrow="ALWAYS"/>
    <JFXButton text="Save" styleClass="save-btn">
        <HBox.margin>
            <Insets left="10.0" />
        </HBox.margin>
    </JFXButton>
    <JFXButton text="Cancel" styleClass="cancel-btn">
        <HBox.margin>
            <Insets left="10.0" />
        </HBox.margin>
    </JFXButton>
    <JFXButton text="Apply" styleClass="apply-btn">
        <HBox.margin>
            <Insets left="10.0" right="5.0"/>
        </HBox.margin>
    </JFXButton>
</HBox>